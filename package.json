{
  "name": "geograph",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "react-router build && cp server.js build/server.js && cp -r app/server app/types app/data app/utils build/",
    "dev": "concurrently \"npm run dev:websocket\" \"react-router dev\"",
    "dev:network": "concurrently \"npm run dev:websocket\" \"react-router dev --host\"",
    "dev:websocket": "tsx app/server/index.ts",
    "start": "concurrently \"npm run start:websocket\" \"react-router-serve ./build/server/index.js\"",
    "start:railway": "tsx build/server.js",
    "start:websocket": "tsx app/server/index.ts",
    "typecheck": "react-router typegen && tsc",
    "test": "vitest",
    "test:ui": "vitest --ui"
  },
  "dependencies": {
    "@react-router/node": "^7.5.3",
    "@react-router/serve": "^7.5.3",
    "@types/better-sqlite3": "^7.6.13",
    "@types/qrcode": "^1.5.5",
    "@types/uuid": "^10.0.0",
    "@types/ws": "^8.18.1",
    "better-sqlite3": "^12.2.0",
    "concurrently": "^9.2.0",
    "http-proxy-middleware": "^3.0.3",
    "isbot": "^5.1.27",
    "leaflet": "^1.9.4",
    "qrcode": "^1.5.4",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-router": "^7.5.3",
    "tsx": "^4.20.3",
    "uuid": "^11.1.0",
    "ws": "^8.18.3"
  },
  "devDependencies": {
    "@react-router/dev": "^7.5.3",
    "@tailwindcss/vite": "^4.1.4",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@types/leaflet": "^1.9.19",
    "@types/node": "^20",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "jsdom": "^26.1.0",
    "prettier": "3.6.2",
    "tailwindcss": "^4.1.4",
    "typescript": "^5.8.3",
    "vite": "^6.3.3",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.2.4"
  }
}
